@model System.Collections.Generic.List<System.Guid>

@{
    Layout = null;

    var loaded = DateTime.Now;
    var singular = Model.Count == 1;
    var verb = singular ? "was" : "were";
    var action = "command" + (singular ? String.Empty : "s");
}

This page loaded <abbr class="timeago" title="@loaded">@loaded</abbr>, at that time there @verb @Model.Count pending @action:
@foreach (var id in Model)
{
<br />@id
}
